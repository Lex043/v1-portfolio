---
import Layout from "../../layouts/Layout.astro";
import { Icon } from "astro-icon/components";
const pageTitle = "Huele Bien Website Devlopement";
---

<Layout pageTitle={pageTitle}>
    <section class="px-6 py-20 md:px-14 lg:px-24 max-w-4xl mx-auto">
        <div class="mt-20">
            <a href="/">
                <Icon
                    class="text-[var(--heading_1_color)]"
                    size={20}
                    name="back"
                />
            </a>
            <div class="mt-10">
                <h1
                    class="uppercase text-[var(--heading_1_color)] text-2xl lg:text-6xl"
                >
                    Huele Bien
                </h1>
                <h3 class="text-[var(--text-color)] my-4">September 2025</h3>
                <p class="text-[var(--text-color)]">
                    Link to the website: <a
                        class="underline"
                        href="https://huelebienbyprudent.com"
                        target="_blank"
                        rel="noopener noreferrer">Huele Bien</a
                    >
                </p>
            </div>
        </div>
    </section>
</Layout>
